{
  "name": "vkirienko-site",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
    "build": "ng build",
    "postbuild": "imagemin src/assets/img/* --plugin.webp.quality=100 --out-dir=dist/VKirienko.Web.Client/assets/img-min/",
    "eslint": "eslint \"src/**/*.ts\"",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && ng serve -o",
    "ncu": "npm-check-updates -u"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.0.9",
    "@angular/cdk": "17.0.5",
    "@angular/common": "17.0.9",
    "@angular/compiler": "17.0.9",
    "@angular/core": "17.0.9",
    "@angular/forms": "17.0.9",
    "@angular/material": "^17.0.5",
    "@angular/platform-browser": "17.0.9",
    "@angular/platform-browser-dynamic": "17.0.9",
    "@angular/platform-server": "17.0.9",
    "@angular/router": "17.0.9",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@microsoft/applicationinsights-web": "^3.0.7",
    "@microsoft/signalr": "^8.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "arrive": "^2.4.1",
    "bootstrap": "4.6.1",
    "bootstrap-material-design": "4.1.3",
    "bootstrap-notify": "3.1.3",
    "chartist": "1.3.0",
    "googleapis": "130.0.0",
    "hammerjs": "2.0.8",
    "jquery": "3.7.1",
    "moment": "2.30.1",
    "perfect-scrollbar": "1.5.5",
    "popper.js": "1.16.1",
    "rxjs": "7.8.1",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular-devkit/schematics": "^17.0.10",
    "@angular-eslint/eslint-plugin": "^17.2.0",
    "@angular/cli": "^17.0.10",
    "@angular/compiler-cli": "17.0.9",
    "@angular/language-service": "17.0.9",
    "@types/bootstrap": "5.2.10",
    "@types/google.maps": "3.54.10",
    "@types/jquery": "3.5.29",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "imagemin-webp": "^8.0.0",
    "npm-check-updates": "^16.14.12",
    "run-script-os": "^1.1.6",
    "ts-node": "10.9.2",
    "typescript": "5.2.2"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
